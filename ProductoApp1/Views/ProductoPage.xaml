<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProductoApp1.ProductoPage"
             Title="ProductoPage">
    <VerticalStackLayout>

        <Button WidthRequest="200" HeightRequest="10" Margin="0,30,0,0" BackgroundColor="Black" CornerRadius="20" Padding="0"
     Text="Iniciar Sesion"
     TextColor="White"
     FontAttributes="Bold"
     Clicked="OnClickIniciarSesion"/>

        <Button WidthRequest="200" HeightRequest="10" Margin="0,30,0,0" BackgroundColor="Black" CornerRadius="20" Padding="0"
         Text="Registrarse"
         TextColor="White"
         FontAttributes="Bold"
         Clicked="OnClickRegistrarse"
        />
        

    </VerticalStackLayout>
</ContentPage>